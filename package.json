{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "npm i && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "npm i && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-price-tracker": "cd nova-components/PriceTracker && npm run dev",
        "build-price-tracker-prod": "cd nova-components/PriceTracker && npm run prod",
        "build-import-payments": "cd nova-components/ImportPayments && npm run dev",
        "build-import-payments-prod": "cd nova-components/ImportPayments && npm run prod",
        "build-dispute-info-panel": "cd nova-components/DisputeInfoPanel && npm run dev",
        "build-dispute-info-panel-prod": "cd nova-components/DisputeInfoPanel && npm run prod",
        "build-account-dispute-info-panel": "cd nova-components/AccountDisputeInfoPanel && npm run dev",
        "build-account-dispute-info-panel-prod": "cd nova-components/AccountDisputeInfoPanel && npm run prod",
        "build-customer-splat": "cd nova-components/CustomerSplat && npm run dev",
        "build-customer-splat-prod": "cd nova-components/CustomerSplat && npm run prod",
        "build-payment-deliverables-info-panel": "cd nova-components/PaymentDeliverablesInfoPanel && npm run dev",
        "build-payment-deliverables-info-panel-prod": "cd nova-components/PaymentDeliverablesInfoPanel && npm run prod",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/preset-env": "^7.21.5",
        "@vue/test-utils": "^1.3.5",
        "axios": "^0.18",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^29.5.0",
        "bootstrap": "^4.1.0",
        "cross-env": "^5.1",
        "jest-serializer-vue": "^3.1.0",
        "jquery": "^3.2",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@fingerprintjs/fingerprintjs-pro": "^3.8.1",
        "bootstrap-vue": "^2.15.0",
        "country-list": "^2.2.0",
        "es6-promise": "^4.2.8",
        "iframe-resizer": "^4.3.2",
        "jest": "^27.5.1",
        "js-cookie": "^2.2.1",
        "vue-axios": "^2.1.4",
        "vue-i18n": "^8.22.4",
        "vue-jest": "^3.0.7",
        "vue-router": "^3.1.2",
        "vue-select": "^3.7.0",
        "vue-sweetalert2": "^2.1.4",
        "vuex": "^3.6.2"
    }
}
